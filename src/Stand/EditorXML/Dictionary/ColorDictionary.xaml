<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="BackgroundColor" Color="#fff"></SolidColorBrush>
    <SolidColorBrush x:Key="ControlsBackground" Color="#fff"></SolidColorBrush>
    <SolidColorBrush x:Key="ControlsBorderBrush" Color="#b1b4bd"></SolidColorBrush>
    <SolidColorBrush x:Key="green" Color="#81E979"></SolidColorBrush>
    <LinearGradientBrush x:Key="greenGradient" StartPoint="0, 0" EndPoint="0, 2">
        <GradientStop Color="#81E979" Offset="0" /> 
        <GradientStop Color="#000000" Offset="1" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="redGradient" StartPoint="0, 0" EndPoint="0, 2">
        <GradientStop Color="#d22431" Offset="0" />
        <GradientStop Color="#000000" Offset="1" />
    </LinearGradientBrush>
    <SolidColorBrush x:Key="whiteForeground" Color="#fff"></SolidColorBrush>
    <SolidColorBrush x:Key="grayBrush" Color="#e1e4e8"></SolidColorBrush>
</ResourceDictionary>